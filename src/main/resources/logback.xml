<?xml version="1.0" encoding="UTF-8" ?>
<configuration>
    <appender name="consoleLog" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>
                %d{yyyy-MM-dd HH:mm:ss.SSS} - %5level [%15.15t] %-30.30C{1.} : %m%n
            </pattern>
        </encoder>
    </appender>
    <root level="info">
        <appender-ref ref="consoleLog"></appender-ref>
    </root>
</configuration>